{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceBaseName": {
      "value": "bot${{RESOURCE_SUFFIX}}"
    },
    "azureOpenAIEndpoint": {
      "value": "${{AZURE_OPENAI_ENDPOINT}}"
    },
    "azureOpenAIDeploymentName": {
      "value": "${{AZURE_OPENAI_DEPLOYMENT_NAME}}"
    },
    "useManagedIdentity": {
      "value": "${{USE_MANAGED_IDENTITY}}"
    },
    "webAppSKU": {
      "value": "B1"
    },
    "botDisplayName": {
      "value": "TravelAgent"
    },
    "environment": {
      "value": "dev"
    },
    "aadAppClientId": {
      "value": "${{AAD_APP_CLIENT_ID}}"
    },
    "aadAppClientSecret": {
      "value": "${{SECRET_AAD_APP_CLIENT_SECRET}}"
    },
    "aadAppTenantId": {
      "value": "${{AAD_APP_TENANT_ID}}"
    },
    "deployVNet": {
      "value": true
    },
    "deployFirewall": {
      "value": false
    },
    "deployKeyVault": {
      "value": true
    },
    "azureOpenAIResourceName": {
      "value": "aif-travelagent-bot"
    },
    "publisherEmail": {
      "value": "admin@travelagent.com"
    },
    "publisherName": {
      "value": "Travel Agent Bot"
    },
    "apimSku": {
      "value": "Developer"
    }
  }
}
